{
  "sourceDataSet": {
    "@xmlns:common": "str",
    "@xmlns": "str",
    "@xmlns:xsi": "str",
    "@version":{
      "value": "str",
      "rules":[
        {
          "required": true,
          "messageKey": "pages.source.validator.version.required",
          "defaultMessage": "version is required"
        }
      ]
    },
    "@xsi:schemaLocation": "str",
    "sourceInformation": {
      "dataSetInformation": {
        "common:UUID": {
          "value":"str",
          "rules":[
            {
              "required": true,
              "messageKey": "pages.source.validator.uuid.required",
              "defaultMessage": "uuid is required"
            }
          ]
        },
        "common:shortName": {
          "value":{
            "@xml:lang": "str",
            "#text": "str"
          },
          "rules":[
            {
              "required": true,
              "messageKey": "pages.source.validator.shortName.required",
              "defaultMessage": "shortName is required"
            },
            {
              "warningOnly": true,
              "messageKey": "validator.recommended.empty",
              "defaultMessage": "Recommended to fill in to enhance data detail and usability!"
            },
            {
              "max": 500,
              "messageKey": "validator.StringMultiLang.pattern",
              "defaultMessage": "Length cannot exceed 500 characters!"
            }
          ]
        },
        "classificationInformation": {
          "common:classification": {
            "common:class":{
              "value": {
                "@level": "str",
                "@classId": "str",
                "#text": "str"
              },
              "rules":[
                {
                  "required": true,
                  "messageKey": "pages.source.validator.class.required",
                  "defaultMessage": "class is required"
                }
              ]
            }
          }
        },
        "sourceCitation": {
          "value": "str",
          "rules": [
            {
              "warningOnly": true,
              "messageKey": "validator.recommended.empty",
              "defaultMessage": "Recommended to fill in to enhance data detail and usability!"
            },
            {
              "max": 1000,
              "messageKey": "validator.STMultiLang.pattern",
              "defaultMessage": "Length cannot exceed 1000 characters!"
            }
          ]
        },
        "publicationType": "str",
        "sourceDescriptionOrComment": [
          {
            "@xml:lang": "str",
            "#text": "str"
          }
        ],
        "referenceToDigitalFile": {
          "@uri": "str" 
        },
        "referenceToContact": {
          "@type": "str",
          "@refObjectId": "str",
          "@version": "str",
          "@uri": "str",
          "common:shortDescription": {
            "@xml:lang": "str",
            "#text": "str"
          }
        },
        "referenceToLogo": {
          "@type": "str",
          "@refObjectId": "str",
          "@version": "str",
          "@uri": "str",
          "common:shortDescription": {
            "@xml:lang": "str",
            "#text": "str"
          }
        }
      }
    },
    "administrativeInformation": { 
      "dataEntryBy": { 
        "common:timeStamp": {
          "value":"str",
          "rules":[
            {
              "required": true,
              "messageKey": "pages.source.validator.timeStamp.required",
              "defaultMessage": "timeStamp is required"
            }
          ]
        }, 
        "common:referenceToDataSetFormat": {
          "value":{
            "@type": "str",
            "@refObjectId": "str",
            "@version": "str",
            "@uri": "str",
            "common:shortDescription": {
              "@xml:lang": "str",
              "#text": "str"
            }
          },
          "rules":[
            {
              "required": true,
              "messageKey": "pages.source.validator.referenceToDataSetFormat.required",
              "defaultMessage": "referenceToDataSetFormat is required"
            }
          ]
        }
      },
      "publicationAndOwnership": {
        "common:dataSetVersion": {
          "value":"str",
          "rules":[
            {
              "required": true,
              "messageKey": "pages.source.validator.dataSetVersion.required",
              "defaultMessage": "dataSetVersion is required"
            },
            {
              "pattern": "dataSetVersion",
              "messageKey": "validator.dataSetVersion.pattern",
              "defaultMessage": "Version format must be XX.XX.XXX, where X is a digit!"
            }
          ]
        },
        "common:referenceToPrecedingDataSetVersion": {
          "@type": "str",
          "@refObjectId": "str",
          "@version": "str",
          "@uri": "str",
          "common:shortDescription": {
            "@xml:lang": "str",
            "#text": "str"
          }
        },
        "common:permanentDataSetURI": "str",
        "common:referenceToOwnershipOfDataSet":{
          "value": {
            "@type": "str",
            "@refObjectId": "str",
            "@version": "str",
            "@uri": "str",
            "common:shortDescription": {
              "@xml:lang": "str",
              "#text": "str"
            }
          },
          "rules":[
            {
              "required": true,
              "messageKey": "pages.source.validator.referenceToOwnershipOfDataSet.required",
              "defaultMessage": "referenceToOwnershipOfDataSet is required"
            }
          ]
        }
      }
    }
  },
  "source:sourceDataSet": {
    "@xmlns:source": "str",
    "@xmlns:common": "str",
    "@xmlns:xsi": "str",
    "@version": "str",
    "@xsi:schemaLocation": "str",
    "source:sourceInformation": {
      "source:dataSetInformation": {
        "common:UUID": "str",
        "common:shortName": {
          "@xml:lang": "str",
          "#text": "str"
        },
        "source:classificationInformation": {
          "common:classification": {
            "common:class": {
              "@level": "str",
              "#text": "str"
            }
          }
        },
        "source:sourceCitation": "str",
        "source:publicationType": "str",
        "source:sourceDescriptionOrComment": {
          "@xml:lang": "str",
          "#text": "str"
        }
      }
    },
    "source:administrativeInformation": {
      "source:dataEntryBy": {
        "common:timeStamp": "str",
        "common:referenceToDataSetFormat": {
          "@refObjectId": "str",
          "@type": "str",
          "@uri": "str",
          "common:shortDescription": {
            "@xml:lang": "str",
            "#text": "str"
          }
        }
      },
      "source:publicationAndOwnership": {
        "common:dataSetVersion": "str",
        "common:permanentDataSetURI": "str"
      }
    }
  },
  "s:sourceDataSet": {
    "@xmlns:s": "str",
    "@xmlns": "str",
    "@xmlns:c": "str",
    "@xmlns:common": "str",
    "@xmlns:f": "str",
    "@xmlns:fp": "str",
    "@xmlns:m": "str",
    "@xmlns:u": "str",
    "@version": "str",
    "s:sourceInformation": {
      "s:dataSetInformation": {
        "common:UUID": "str",
        "common:shortName": {
          "@xml:lang": "str",
          "#text": "str"
        },
        "s:classificationInformation": "NoneType",
        "s:sourceDescriptionOrComment": {
          "@xml:lang": "str"
        }
      }
    },
    "s:administrativeInformation": {
      "s:dataEntryBy": {
        "common:timeStamp": "str",
        "common:referenceToDataSetFormat": {
          "@refObjectId": "str",
          "@type": "str",
          "@uri": "str",
          "@version": "str",
          "common:shortDescription": {
            "@xml:lang": "str",
            "#text": "str"
          }
        }
      },
      "s:publicationAndOwnership": {
        "common:dataSetVersion": "str"
      }
    }
  }
}
