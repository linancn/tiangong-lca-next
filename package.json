{
  "name": "tiangong-lca-next",
  "version": "0.0.17",
  "private": true,
  "description": "An out-of-box LCA Data solution",
  "homepage": "./",
  "license": "MIT",
  "author": "TianGong LCA <linanenv@gmail.com> (https://lca.tiangong.earth)",
  "main": "dist-electron/main.js",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "dist": "npm run build && tsc --project tsconfig.electron.json && electron-builder --config electron-builder.json",
    "electron": "tsc --project tsconfig.electron.json && electron dist-electron/main.js",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "jest": "jest",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=stylish ./src ./tests",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=stylish ./src ./tests",
    "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "openapi": "max openapi",
    "prepare": "husky",
    "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "preview": "npm run build && max preview --port 8000",
    "record": "cross-env NODE_ENV=development REACT_APP_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "test": "cross-env CI=true npm run test:ci",
    "test:unit": "jest tests/unit src --testTimeout=20000",
    "test:integration": "jest tests/integration --runInBand --testTimeout=20000",
    "test:ci": "node scripts/test-runner.cjs",
    "test:coverage": "jest --runInBand --testTimeout=20000 --coverage",
    "test:coverage:report": "jest --runInBand --testTimeout=20000 --coverage; node scripts/test-coverage-report.js",
    "test:report": "npm run test:coverage:report",
    "test:update": "jest --runInBand --testTimeout=20000 -u",
    "tsc": "tsc --noEmit",
    "test:antchain": "npx tsx src/services/antchain/api.ts",
    "ncu": "npx npm-check-updates",
    "ncu:update": "npx npm-check-updates -u"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/pro-components": "^2.8.10",
    "@ant-design/pro-table": "^3.21.0",
    "@antv/x6": "^2.19.2",
    "@antv/x6-plugin-selection": "^2.2.2",
    "@antv/x6-plugin-snapline": "^2.1.7",
    "@antv/x6-plugin-transform": "^2.1.8",
    "@antv/x6-react-shape": "^2.2.3",
    "@supabase/supabase-js": "^2.90.1",
    "@tiangong-lca/tidas-sdk": "^0.1.28",
    "antd": "^5.29.1",
    "antd-style": "^3.7.1",
    "bignumber.js": "^9.3.1",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.19",
    "jsondiffpatch": "^0.7.3",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.1",
    "@umijs/lint": "^4.6.22",
    "@umijs/max": "^4.6.22",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "eslint": "^8.57.1",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.7",
    "npm-check-updates": "^19.3.1",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "react-dev-inspector": "^2.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "umi-presets-pro": "^2.0.3",
    "umi-serve": "^1.9.11"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
